					<layoutData>
						<l:GridData span="L2 M2 S2" />
					</layoutData>											
				</CheckBox>	
				
				<Label id="taxCodeLabel" 
					text="{i18n>taxCode}" 
					required="true"
					visible="{utils>/freelanceButtonSelected}" />
